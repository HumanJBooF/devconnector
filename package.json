{
  "name": "devconnector",
  "version": "1.0.0",
  "description": "Social Developer Network",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client-install": "npm install --prefix client",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "engines": {
    "node": "8.11.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HumanJBooF/devconnector.git"
  },
  "author": "Joshua LeBoeuf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HumanJBooF/devconnector/issues"
  },
  "homepage": "https://github.com/HumanJBooF/devconnector#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}